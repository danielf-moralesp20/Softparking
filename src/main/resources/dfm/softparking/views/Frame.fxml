<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>


<ScrollPane fitToHeight="true" fitToWidth="true" pannable="true" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <VBox fx:id="rootContainer" alignment="TOP_CENTER" minHeight="720.0" minWidth="1130.0" prefHeight="720.0" prefWidth="1280.0" stylesheets="@../style/root.css">
         <children>
            <AnchorPane prefHeight="45.0" prefWidth="1280.0" style="-fx-background-color: #FFFFFF;" VBox.vgrow="NEVER">
               <children>
                  <Button layoutX="922.0" layoutY="53.0" mnemonicParsing="false" onAction="#btnMinimizeEventOnAction" AnchorPane.rightAnchor="45.0" AnchorPane.topAnchor="7.0">
                     <graphic>
                        <FontIcon iconColor="WHITE" iconLiteral="mdi-minus-circle-outline" iconSize="17" />
                     </graphic>
                  </Button>
                  <Button layoutX="912.0" layoutY="43.0" mnemonicParsing="false" onAction="#btnCloseEventOnAction" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="7.0">
                     <graphic>
                        <FontIcon iconColor="WHITE" iconLiteral="mdi-close-circle-outline" iconSize="17" />
                     </graphic>
                  </Button>
               </children>
            </AnchorPane>
            <AnchorPane fx:id="mainYield" prefHeight="676.0" prefWidth="1280.0" style="-fx-background-color: #FFFFFF;" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </content>
</ScrollPane>
