<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<VBox fx:id="globalContainer" alignment="TOP_CENTER" style="-fx-background-color: #FFFFFF;" stylesheets="@../style/root.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane prefHeight="45.0" prefWidth="1280.0" VBox.vgrow="NEVER">
         <children>
            <Button layoutX="922.0" layoutY="53.0" mnemonicParsing="false" onAction="#btnMinimizeEventOnAction" AnchorPane.rightAnchor="45.0" AnchorPane.topAnchor="7.0">
               <graphic>
                  <FontIcon iconColor="WHITE" iconLiteral="mdi-minus-circle-outline" iconSize="17" />
               </graphic>
            </Button>
            <Button layoutX="912.0" layoutY="43.0" mnemonicParsing="false" onAction="#btnCloseEventOnAction" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="7.0">
               <graphic>
                  <FontIcon iconColor="WHITE" iconLiteral="mdi-close-circle-outline" iconSize="17" />
               </graphic>
            </Button>
         </children>
      </AnchorPane>
      <StackPane fx:id="mainContainer" prefHeight="676.0" prefWidth="1280.0" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
